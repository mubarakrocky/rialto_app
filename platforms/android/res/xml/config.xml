<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.hellocordova" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="SplashScreen">
        <param name="android-package" value="org.apache.cordova.splashscreen.SplashScreen" />
        <param name="onload" value="true" />
    </feature>
    <splash density="land-hdpi" src="platforms/android/res/drawable-hdpi/hdpi.png" />
    <splash density="land-ldpi" src="platforms/android/res/drawable-ldpi/ldpi.png" />
    <splash density="land-mdpi" src="platforms/android/res/drawable-mdpi/mdpi.png" />
    <splash density="land-xhdpi" src="platforms/android/res/drawable-xhdpi/xhdpi.png" />
    <splash density="port-hdpi" src="platforms/android/res/drawable-hdpi/hdpi.png" />
    <splash density="port-ldpi" src="platforms/android/res/drawable-ldpi/ldpi.png" />
    <splash density="port-mdpi" src="platforms/android/res/drawable-mdpi/mdpi.png" />
    <splash density="port-xhdpi" src="platforms/android/res/drawable-xhdpi/xhdpi.png" />
    <name>Fishcart Logistics</name>
    <description>
        Fishcart logistics utility app
    </description>
    <author email="mubarakrocky@gmail.com" href="http://mubarakrocky.tumblr.com">
        Mubarak Rocky
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="SplashScreenDelay" value="1000" />
    <preference name="SplashMaintainAspectRatio" value="true" />
</widget>
